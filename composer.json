{
  "name": "glebred/search-multiselect-input",
  "description": "Searchable multiple select input for Livewire Laravel 8",
  "type": "library",
  "keywords": [
    "glebred",
    "livewire-select",
    "search"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gleb Redko",
      "email": "gleb@redko.ninja"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.0|^9.0",
    "livewire/livewire": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "GlebRed\\SearchMultiselectInput\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GlebRed\\SearchMultiselectInput\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "GlebRed\\SearchMultiselectInput\\SearchMultiselectInputServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^5.0|^6.0",
    "phpunit/phpunit": "^8.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  }
}
